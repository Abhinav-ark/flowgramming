{
    "name": "@quintype/copyright-header",
    "version": "1.3.3",
    "description": "To manage copyright headers in source files",
    "bin": {
        "copyright-header": "dist/src/index.js"
    },
    "main": "dist/src/index.js",
    "scripts": {
        "start": "node dist/src/index.js",
        "prestart": "npm run build",
        "build": "tsc",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "pretest": "npm run build",
        "test": "node dist/src/index.js -i \"test/files/**/*\" -f test/header.js"
    },
    "author": "Quintype",
    "license": "ISC",
    "repository": "https://github.com/quintype/copyright-header",
    "engines": {
        "node": ">=10.23.3"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "commander": "^7.1.0",
        "fast-glob": "^3.2.5",
        "replace-in-file": "^6.2.0",
        "table": "^6.0.7"
    },
    "devDependencies": {
        "@types/node": "^14.14.26",
        "@types/table": "^6.0.0",
        "prettier": "^2.2.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.1.5"
    }
}
